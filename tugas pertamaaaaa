#include <iostream>
using namespace std;

int main() {
    int nilai_mahasiswa[5];
    float total = 0;

    cout << "Silahkan input 5 nilai mahasiswa anda: ";

    for (int i = 0; i < 5; i++) {
        cin >> nilai_mahasiswa[i];
        total += nilai_mahasiswa[i];
    }

    cout << "Rata-rata nilai mahasiswa anda: " << total / 5 << endl;

    return 0;
}
